@font-face {
  font-family: 'Bookerly-Bold';
  src: url('../fonts/BookerlyBold/Bookerly-Bold.eot');
  src: url('../fonts/BookerlyBold/Bookerly-Bold.woff') format('woff'),
       url('../fonts/BookerlyBold/Bookerly-Bold.ttf') format('truetype'),
       url('../fonts/BookerlyBold/Bookerly-Bold.eot?#iefix') format('embedded-opentype');
  font-weight: normal;
  font-style: normal;
}

